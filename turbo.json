{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "inputs": [
                "$TURBO_DEFAULT$",
                ".env*"
            ],
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "build/**",
                ".vercel/**",
                "dist/**",
                ".next/**",
                "!.next/cache/**"
            ]
        },
        "test": {
            "outputs": [
                "coverage/**"
            ],
            "dependsOn": []
        },
        "lint": {
            "dependsOn": [
                "^lint"
            ]
        },
        "inspect": {
            "dependsOn": [
                "^inspect"
            ]
        },
        "typecheck": {
            "dependsOn": [
                "^build"
            ]
        },
        "dev": {
            "dependsOn": [
                "^build"
            ],
            "cache": false,
            "persistent": true
        },
        "clean": {
            "cache": false
        },
        "transit": {
            "dependsOn": [
                "^transit"
            ]
        },
        "check-types": {
            "dependsOn": [
                "transit"
            ]
        },
        "check": {
            "dependsOn": [
                "transit"
            ]
        }
    }
}